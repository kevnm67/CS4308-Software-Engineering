GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/CocoaLumberjack.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Organic.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/RMActionController.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/RMPickerViewController.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "CocoaLumberjack" -framework "Organic" -framework "RMActionController" -framework "RMPickerViewController"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods
PODS_ROOT = ${SRCROOT}/Pods